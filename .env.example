# Environment: dev | prod
BUN_ENV=dev

# Server port
PORT=8000

# Meta App ID from Facebook Developers Console
META_APP_ID=your_app_id_here

# System User Access Token (keep this secret!)
META_SYSTEM_USER_TOKEN=your_system_user_token_here

# Meta Graph API version (without 'v' prefix)
META_GRAPH_VERSION=24.0
